# Rename this file to .env and fill in your actual API keys and config values.

######################################
# ‚úÖ Language Model API Keys & URLs
######################################

# Groq API Key - https://console.groq.com/keys
GROQ_API_KEY=

# HuggingFace API Key - https://huggingface.co/settings/tokens
HuggingFace_API_KEY=

# OpenAI API Key - https://platform.openai.com/account/api-keys
OPENAI_API_KEY=

# Anthropic (Claude) API Key - https://console.anthropic.com/settings/keys
ANTHROPIC_API_KEY=

# OpenRouter API Key - https://openrouter.ai/settings/keys
OPEN_ROUTER_API_KEY=

# Google Generative AI API Key - https://console.cloud.google.com/apis/credentials
GOOGLE_GENERATIVE_AI_API_KEY=

# Ollama API Base URL (local) - e.g. http://127.0.0.1:11434
OLLAMA_API_BASE_URL=

# OpenAI-Compatible API Base URL (e.g. custom LLM backend)
OPENAI_LIKE_API_BASE_URL=

# Together AI API Base URL - e.g. https://api.together.xyz/v1
TOGETHER_API_BASE_URL=

# DeepSeek API Key (if using via direct API)
DEEPSEEK_API_KEY=

# OpenAI-Compatible API Key (for services like Groq/OpenRouter)
OPENAI_LIKE_API_KEY=

# Together API Key - https://www.together.ai
TOGETHER_API_KEY=

# Hyperbolic AI Key - https://app.hyperbolic.xyz/settings
HYPERBOLIC_API_KEY=
HYPERBOLIC_API_BASE_URL=

# Mistral API Key - https://console.mistral.ai/api-keys/
MISTRAL_API_KEY=

# Cohere API Key - https://dashboard.cohere.com/api-keys
COHERE_API_KEY=

# LMStudio Local API Base URL - e.g. http://127.0.0.1:1234
LMSTUDIO_API_BASE_URL=

# xAI API Key - https://x.ai/api
XAI_API_KEY=

# Perplexity API Key - https://www.perplexity.ai/settings/api
PERPLEXITY_API_KEY=

######################################
# ‚òÅÔ∏è AWS Bedrock Config (JSON string)
######################################
# Example format:
# {"region": "us-east-1", "accessKeyId": "yourAccessKeyId", "secretAccessKey": "yourSecretAccessKey", "sessionToken": "yourSessionToken"}
AWS_BEDROCK_CONFIG=

######################################
# üõ†Ô∏è Developer Tools
######################################

# Log level for local dev debugging
VITE_LOG_LEVEL=debug

# GitHub Personal Access Token - https://github.com/settings/tokens
VITE_GITHUB_ACCESS_TOKEN=

# Type of GitHub token: 'classic' or 'fine-grained'
VITE_GITHUB_TOKEN_TYPE=classic

# Default context size for models like Qwen
DEFAULT_NUM_CTX=12288
